<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>jsp_02_servlet</display-name>
  <welcome-file-list>
    <welcome-file>main/home.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>first.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>ChoiceFruitsServlet</display-name>
    <servlet-name>ChoiceFruitsServlet</servlet-name>
    <servlet-class>first.ChoiceFruitsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChoiceFruitsServlet</servlet-name>
    <url-pattern>/choiceFruit</url-pattern>
  </servlet-mapping>
</web-app>